{
    "name": "lotltd/shotstack-sdk-php",
    "description": "PHP SDK for Shotstack, the cloud video editing API",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "keywords": ["shotstack", "video", "video editing", "api"],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "*"
    },
    "autoload": {
        "psr-4": { "Shotstack\\Client\\" : "src/" }
    }
}
